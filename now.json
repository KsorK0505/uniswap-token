{
  "name": "uniswap-info-experimental",
  "version": 2,
  "alias": "api.uniswap.info",
  "builds": [{ "src": "./src/**/*.ts", "use": "@now/node" }],
  "routes": [
    { "src": "/status", "dest": "/src/status", "methods": ["GET"] },
    { "src": "/tokens", "dest": "/src/tokens", "methods": ["GET"] },
    { "src": "/token/(?<tokenAddress>.*)", "dest": "/src/token?tokenAddress=$tokenAddress", "methods": ["GET"] }
  ]
}
