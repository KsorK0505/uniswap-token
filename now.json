{
  "name": "uniswap-info-experimental",
  "version": 2,
  "alias": "api.uniswap.info",
  "builds": [{ "src": "./src/**/*.ts", "use": "@now/node" }],
  "routes": [
    { "src": "/status", "dest": "/src/status", "methods": ["GET"] },
    { "src": "/pairs", "dest": "/src/pairs", "methods": ["GET"] },
    { "src": "/pair/(?<tokenAddress>.*)", "dest": "/src/pair?tokenAddress=$tokenAddress", "methods": ["GET"] },
    { "src": "/pink", "dest": "/src/pink.ts", "methods": ["GET"] },
    { "src": "/%F0%9F%A6%84", "dest": "/src/ðŸ¦„.ts", "methods": ["GET"] },
    { "src": "/.*", "dest": "/404.html", "status": 404 }
  ]
}
